    rdfs:domain :DescriptionProblematique ;
    rdfs:range :MultimediaProblematique ;
    rdfs:label "associe ressource problématique" ;
    rdfs:comment "Images associées à une problématique client." .

:flyerAssocieRessource a owl:ObjectProperty ;
    rdfs:domain :Flyer ;
    rdfs:range :MultimediaFlyer ;
    rdfs:label "associe ressource flyer" ;
    rdfs:comment "Ressources visuelles incluses dans un flyer." .

:textRessourceObjet a owl:DatatypeProperty ;
    rdfs:subPropertyOf :textRessource ;
    rdfs:domain :MultimediaObjet ;
    rdfs:range xsd:string ;
    rdfs:label "texte ressource objet" ;
    rdfs:comment "Texte descriptif associé à la ressource multimédia d’objet." .

:textRessourceconclusion a owl:DatatypeProperty ;
    rdfs:subPropertyOf :textRessource ;
    rdfs:domain :MultimediaConclusion ;
    rdfs:range xsd:string ;
    rdfs:label "texte ressource conclusion" ;
    rdfs:comment "Texte descriptif associé à la ressource multimédia conclusion." .

:textRessourceResultat a owl:DatatypeProperty ;
    rdfs:subPropertyOf :textRessource ;
    rdfs:domain :MultimediaResultat ;
    rdfs:range xsd:string ;
    rdfs:label "texte ressource résultat" ;
    rdfs:comment "Texte descriptif associé à la ressource multimédia de résultat." .

:textRessourceAnnexe a owl:DatatypeProperty ;
    rdfs:subPropertyOf :textRessource ;
    rdfs:domain :MultimediaAnnexe ;
    rdfs:range xsd:string ;
    rdfs:label "texte ressource annexe" ;
    rdfs:comment "Texte descriptif associé à la ressource multimédia d'annexe." .

:textRessourceFlyer a owl:DatatypeProperty ;
    rdfs:subPropertyOf :textRessource ;
    rdfs:domain :MultimediaFlyer ;
    rdfs:range xsd:string ;
    rdfs:label "texte ressource flyer" ;
    rdfs:comment "Texte descriptif associé à la ressource multimédia de flyer." .

:textRessourceProblematique a owl:DatatypeProperty ;
    rdfs:subPropertyOf :textRessource ;
    rdfs:domain :MultimediaProblematique ;
    rdfs:range xsd:string ;
    rdfs:label "texte ressource problématique" ;
    rdfs:comment "Texte descriptif associé à la ressource multimédia de problématique." .


:rapportInclutEchantillon a owl:ObjectProperty ;
    rdfs:domain :RapportAnalyse ; rdfs:range :Echantillon ;
    rdfs:label "inclut échantillon" ;
    rdfs:comment "Lien entre un rapport et un échantillon analysé." .


:rapportInclutParametre a owl:ObjectProperty ;
    rdfs:domain :RapportAnalyse ; rdfs:range :ParametreAnalytique ;
    rdfs:label "inclut paramètre" ;
    rdfs:comment "Lien vers les paramètres utilisés lors de l’analyse." .

:rapportProduitResultat a owl:ObjectProperty ;
    rdfs:domain :RapportAnalyse ; rdfs:range :Resultat ;
    rdfs:label "produit résultat" ;
    rdfs:comment "Lien entre un rapport et les résultats générés." .

:rapportInclutConclusion a owl:ObjectProperty ;
    rdfs:domain :RapportAnalyse ; rdfs:range :Conclusion ;
    rdfs:label "inclut conclusion" ;
    rdfs:comment "Lien vers la conclusion du rapport." .



:rapportInclutAnnexe a owl:ObjectProperty ;
    rdfs:domain :RapportAnalyse ; rdfs:range :Annexe ;
    rdfs:label "inclut annexe" ;
    rdfs:comment "Lien vers les annexes du rapport." .




:responsableRapport a owl:DatatypeProperty ;
    rdfs:domain :RapportAnalyse ; rdfs:range xsd:string ;
    rdfs:label "responsable d'analyse ." .

:dateAnalyse a owl:DatatypeProperty ;
    rdfs:domain :Echantillon ; rdfs:range xsd:dateTime ;
    rdfs:label "date d’analyse ." .



:dateReception a owl:DatatypeProperty ;
    rdfs:domain :Echantillon ; rdfs:range xsd:dateTime ;
