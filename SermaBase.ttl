@prefix : <http://data.serma.org/ontoserma#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:DocumentMetier a owl:Class ;
    rdfs:label "Document métier" ;
    rdfs:comment "Classe générale représentant n’importe quel document interne du groupe Serma." .

:RapportAnalyse a owl:Class ;
    rdfs:label "Rapport d'analyse" ;
    rdfs:subClassOf :DocumentMetier ;
    rdfs:comment "Document qui synthétise les résultats d’analyses techniques." .

:Echantillon a owl:Class ;
    rdfs:label "Échantillon" ;
    rdfs:comment "Échantillon analysé (avec date d’analyse, référence, etc.)." .

:ProgrammeAnalyse a owl:Class ;
    rdfs:label "Programme d’analyse" ;
    rdfs:comment "Protocole ou méthodologie utilisée lors d’une analyse, dans un rapport ou un devis." .

:ParametreAnalytique a owl:Class ;
    rdfs:label "Paramètre analytique" ;
    rdfs:comment "Paramètre (température, durée, etc.) contrôlé pendant l’analyse." .

:Resultat a owl:Class ;
    rdfs:label "Résultat" ;
    rdfs:comment "Résultat de l’analyse (texte, tableau, graphe, etc.)." .

:Conclusion a owl:Class ;
    rdfs:label "Conclusion" ;
    rdfs:comment "Conclusion écrite tirée de l’analyse." .


:Annexe a owl:Class ;
    rdfs:label "Annexe" ;
    rdfs:comment "Annexe jointe au rapport contenant des images, spectres, profils, tableaux, etc." .

:RessourceMultimedia a owl:Class ;
    rdfs:label "Ressource multimédia" ;
    rdfs:comment "Image, spectre, profil, ou ressource visuelle utilisée dans un rapport ou une annexe." .

:MultimediaAnnexe a owl:Class ;
    rdfs:subClassOf :RessourceMultimedia ;
    rdfs:label "Ressource multimédia d’annexe" ;
    rdfs:comment "Image ou spectre joint à une annexe de rapport." .

:textRessource a owl:DatatypeProperty ;
    rdfs:domain :RessourceMultimedia ;
    rdfs:range xsd:string ;
    rdfs:label "texte ressource" ;
    rdfs:comment "Texte descriptif associé à la ressource multimédia." .

:MultimediaResultat a owl:Class ;
    rdfs:subClassOf :RessourceMultimedia ;
    rdfs:label "Ressource multimédia de résultat" ;
    rdfs:comment "Graphique, tableau ou image produit comme résultat d'analyse." .

:MultimediaObjet a owl:Class ;
    rdfs:subClassOf :RessourceMultimedia ;
    rdfs:label "Ressource multimédia d'objet" ;
    rdfs:comment "Photo ou ressource liée à un objet analysé." .

:MultimediaConclusion a owl:Class ;
    rdfs:subClassOf :RessourceMultimedia ;
    rdfs:label "Ressource multimédia conclusion" ;
    rdfs:comment "tableau  ou schéma liée au conclusion." .

:MultimediaProblematique a owl:Class ;
    rdfs:subClassOf :RessourceMultimedia ;
    rdfs:label "Ressource multimédia de problématique" ;
    rdfs:comment "Ressource visuelle liée à une problématique client." .

:MultimediaFlyer a owl:Class ;
    rdfs:subClassOf :RessourceMultimedia ;
    rdfs:label "Ressource multimédia de flyer" ;
    rdfs:comment "Ressource visuelle incluse dans un flyer marketing." .

:annexeContientRessource a owl:ObjectProperty ;
    rdfs:domain :Annexe ;
    rdfs:range :MultimediaAnnexe ;
    rdfs:label "contient ressource annexe" ;
    rdfs:comment "Ressources multimédias spécifiques à une annexe." .

:conclusionContientRessource a owl:ObjectProperty ;
    rdfs:domain :Conclusion ;
    rdfs:range :MultimediaConclusion ;
    rdfs:label "contient ressource conclusion" ;
    rdfs:comment "Ressources multimédias spécifiques à une conclusion." .

:resultatContientRessource a owl:ObjectProperty ;
    rdfs:domain :Resultat ;
    rdfs:range :MultimediaResultat ;
    rdfs:label "contient ressource résultat" ;
    rdfs:comment "Ressources produites en tant que résultats d’analyse." .


:objetContientRessource a owl:ObjectProperty ;
    rdfs:domain :ObjetAnalyse ;
    rdfs:range :MultimediaObjet ;
    rdfs:label "contient ressource objet" ;
    rdfs:comment "Ressources liées à l’objet analysé." .

