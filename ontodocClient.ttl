@prefix : <http://data.serma.org/ontodocClient#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

#################################################################
# Classes — Documents Client (FR)
#################################################################

:DocumentClient a owl:Class ;
  rdfs:label "Document client" ;
  rdfs:comment "Document fourni par un client pour initier une demande (demande d'analyse, bon d'envoi, etc.)." .

:Client a owl:Class ;
  rdfs:label "Client" .

:Prestataire a owl:Class ;
  rdfs:label "Prestataire" ;
  rdfs:comment "Laboratoire ou société réalisant les analyses." .

:DemandeAnalyse a owl:Class ;
  rdfs:label "Demande d'analyse" .

:Problematique a owl:Class ;
  rdfs:label "Problématique" ;
  rdfs:comment "Description du problème / motivation de la demande (info la plus importante)." .

:Echantillon a owl:Class ;
  rdfs:label "Échantillon" .

:EtatEchantillon a owl:Class ;
  rdfs:label "État de l'échantillon" .

:Materiau a owl:Class ;
  rdfs:label "Matériau" .

:Lot a owl:Class ;
  rdfs:label "Lot" .

:PlanAnalyse a owl:Class ;
  rdfs:label "Plan d'analyse" .

:AnalyseDemandee a owl:Class ;
  rdfs:label "Analyse demandée" .

:TechniqueAnalyse a owl:Class ;
  rdfs:label "Technique d'analyse" .

:ZoneAnalyse a owl:Class ;
  rdfs:label "Zone d'analyse" .

:LivrableAttendu a owl:Class ;
  rdfs:label "Livrable attendu" .

:ReferenceCommerciale a owl:Class ;
  rdfs:label "Référence commerciale" ;
  rdfs:comment "Devis, commande, référence interne…" .

:Expedition a owl:Class ;
  rdfs:label "Expédition" ;
  rdfs:comment "Informations d'envoi/réception (transport, date, colis, etc.)." .

#################################################################
# Object Properties — Relations (IDs stables + labels FR)
#################################################################

:aPourClient a owl:ObjectProperty ;
  rdfs:label "aPourClient" ;
  rdfs:domain :DocumentClient ;
  rdfs:range  :Client .

:aPourPrestataire a owl:ObjectProperty ;
  rdfs:label "aPourPrestataire" ;
  rdfs:domain :DocumentClient ;
  rdfs:range  :Prestataire .

:decritDemande a owl:ObjectProperty ;
  rdfs:label "decritDemande" ;
  rdfs:domain :DocumentClient ;
  rdfs:range  :DemandeAnalyse .

:aPourProblematique a owl:ObjectProperty ;
  rdfs:label "aPourProblematique" ;
  rdfs:domain :DemandeAnalyse ;
  rdfs:range  :Problematique .

:inclutEchantillon a owl:ObjectProperty ;
  rdfs:label "inclutEchantillon" ;
  rdfs:domain :DemandeAnalyse ;
  rdfs:range  :Echantillon .

:aPourEtat a owl:ObjectProperty ;
  rdfs:label "aPourEtat" ;
  rdfs:domain :Echantillon ;
  rdfs:range  :EtatEchantillon .

:aPourMateriau a owl:ObjectProperty ;
  rdfs:label "aPourMateriau" ;
  rdfs:domain :Echantillon ;
  rdfs:range  :Materiau .

:associeLot a owl:ObjectProperty ;
  rdfs:label "associeLot" ;
  rdfs:comment "Lien souple pour gérer la variabilité (lot côté demande ou côté échantillon)." ;
  rdfs:domain :DemandeAnalyse ;
  rdfs:range  :Lot .

:aPourPlanAnalyse a owl:ObjectProperty ;
  rdfs:label "aPourPlanAnalyse" ;
  rdfs:domain :DemandeAnalyse ;
  rdfs:range  :PlanAnalyse .

:inclutAnalyse a owl:ObjectProperty ;
  rdfs:label "inclutAnalyse" ;
  rdfs:domain :PlanAnalyse ;
  rdfs:range  :AnalyseDemandee .

:utiliseTechnique a owl:ObjectProperty ;
  rdfs:label "utiliseTechnique" ;
  rdfs:domain :AnalyseDemandee ;
  rdfs:range  :TechniqueAnalyse .

:cibleEchantillon a owl:ObjectProperty ;
  rdfs:label "cibleEchantillon" ;
  rdfs:domain :AnalyseDemandee ;
  rdfs:range  :Echantillon .

:cibleZone a owl:ObjectProperty ;
  rdfs:label "cibleZone" ;
  rdfs:domain :AnalyseDemandee ;
  rdfs:range  :ZoneAnalyse .

:attendLivrable a owl:ObjectProperty ;
  rdfs:label "attendLivrable" ;
  rdfs:domain :DemandeAnalyse ;
  rdfs:range  :LivrableAttendu .

:aPourReferenceCommerciale a owl:ObjectProperty ;
  rdfs:label "aPourReferenceCommerciale" ;
  rdfs:domain :DocumentClient ;
  rdfs:range  :ReferenceCommerciale .

:aPourExpedition a owl:ObjectProperty ;
  rdfs:label "aPourExpedition" ;
  rdfs:domain :DocumentClient ;
  rdfs:range  :Expedition .

#################################################################
# Datatype Properties — Champs clés (focus problématique)
#################################################################

:typeDocument a owl:DatatypeProperty ;
  rdfs:label "typeDocument" ;
  rdfs:domain :DocumentClient ;
  rdfs:range xsd:string .

:numeroDocument a owl:DatatypeProperty ;
  rdfs:label "numeroDocument" ;
  rdfs:domain :DocumentClient ;
  rdfs:range xsd:string .

:dateDocument a owl:DatatypeProperty ;
  rdfs:label "dateDocument" ;
  rdfs:domain :DocumentClient ;
  rdfs:range xsd:string .

:descriptionDemande a owl:DatatypeProperty ;
  rdfs:label "descriptionDemande" ;
  rdfs:domain :DemandeAnalyse ;
  rdfs:range xsd:string .

:texteProblematique a owl:DatatypeProperty ;
  rdfs:label "texteProblematique" ;
  rdfs:domain :Problematique ;
  rdfs:range xsd:string .

:nomEchantillon a owl:DatatypeProperty ;
  rdfs:label "nomEchantillon" ;
  rdfs:domain :Echantillon ;
  rdfs:range xsd:string .

:quantiteEchantillon a owl:DatatypeProperty ;
  rdfs:label "quantiteEchantillon" ;
  rdfs:domain :Echantillon ;
  rdfs:range xsd:int .

:nombreRepetitions a owl:DatatypeProperty ;
  rdfs:label "nombreRepetitions" ;
  rdfs:domain :AnalyseDemandee ;
  rdfs:range xsd:int .

:commentairePlan a owl:DatatypeProperty ;
  rdfs:label "commentairePlan" ;
  rdfs:domain :PlanAnalyse ;
  rdfs:range xsd:string .

:descriptionLivrable a owl:DatatypeProperty ;
  rdfs:label "descriptionLivrable" ;
  rdfs:domain :LivrableAttendu ;
  rdfs:range xsd:string .

:numeroDevis a owl:DatatypeProperty ;
  rdfs:label "numeroDevis" ;
  rdfs:domain :ReferenceCommerciale ;
  rdfs:range xsd:string .

:numeroCommande a owl:DatatypeProperty ;
  rdfs:label "numeroCommande" ;
  rdfs:domain :ReferenceCommerciale ;
  rdfs:range xsd:string .

:referenceInterne a owl:DatatypeProperty ;
  rdfs:label "referenceInterne" ;
  rdfs:domain :ReferenceCommerciale ;
  rdfs:range xsd:string .
