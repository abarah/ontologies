:codeClient a owl:DatatypeProperty ;
  rdfs:domain :Client ;
  rdfs:range xsd:string ;
  rdfs:label "code client" .

:numeroClient a owl:DatatypeProperty ;
  rdfs:domain :Client ;
  rdfs:range xsd:string ;
  rdfs:label "numéro client" .

:codeFournisseur a owl:DatatypeProperty ;
  rdfs:domain :Fournisseur ;
  rdfs:range xsd:string ;
  rdfs:label "code fournisseur" .

:codedaffaireproposition a owl:DatatypeProperty ;
  rdfs:domain :PropositionCommerciale ;
  rdfs:range xsd:string ;
  rdfs:label "code d'affaire proposition" .

:codedaffaireDevis a owl:DatatypeProperty ;
  rdfs:domain :Devis ;
  rdfs:range xsd:string ;
  rdfs:label "code d'affaire devis" .

:referenceProposition a owl:DatatypeProperty ;
  rdfs:domain :PropositionCommerciale ;
  rdfs:range xsd:string ;
  rdfs:label "référence proposition" .

:dateEmissionProposition a owl:DatatypeProperty ;
  rdfs:domain :PropositionCommerciale ;
  rdfs:range xsd:dateTime ;
