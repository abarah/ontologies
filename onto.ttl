@prefix : <http://data.serma.org/ontoserma#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

############################################################
# DOCUMENTS
############################################################

:DocumentMetier a owl:Class ;
  rdfs:label "Document métier" ;
  rdfs:comment "Classe générale représentant tout document interne du groupe SERMA." .

:RapportAnalyse a owl:Class ;
  rdfs:label "Rapport d'analyse" ;
  rdfs:subClassOf :DocumentMetier ;
  rdfs:comment "Document qui synthétise les résultats d’analyses techniques." .

:Devis a owl:Class ;
  rdfs:label "Devis" ;
  rdfs:subClassOf :DocumentMetier ;
  rdfs:comment "Document de type devis technique ou commercial." .

:PropositionCommerciale a owl:Class ;
  rdfs:label "Proposition commerciale" ;
  rdfs:subClassOf :DocumentMetier ;
  rdfs:comment "Offre commerciale pouvant précéder ou compléter un devis." .

############################################################
# ACTEURS
############################################################

:Client a owl:Class ;
  rdfs:label "Client" ;
  rdfs:comment "Organisation cliente destinataire des documents." .

:Fournisseur a owl:Class ;
  rdfs:label "Fournisseur" ;
  rdfs:comment "Organisation émettrice (ex: SERMA Technologies)." .

############################################################
# ANALYSE & ÉCHANTILLONS
############################################################

:Echantillon a owl:Class ;
  rdfs:label "Échantillon" ;
  rdfs:comment "Échantillon analysé dans le cadre d’un rapport." .

:AnalysesProposees a owl:Class ;
  rdfs:label "Analyses proposées" ;
  rdfs:comment "Analyses proposées dans un devis ou une proposition commerciale." .


:CorpsEtranger a owl:Class ;
  rdfs:label "Corps étranger" ;
  rdfs:comment "Défaut correspondant à la présence d’un élément indésirable ou exogène dans ou sur un échantillon, un composant ou un objet analysé." .

:InstrumentAnalyse a owl:Class ;
  rdfs:label "Instrument d'analyse" ;
  rdfs:comment "Instrument ou technique d’analyse (FTIR, MEB, XPS…).".

:detailsInstrument a owl:DatatypeProperty ;
  rdfs:domain :InstrumentAnalyse ;
  rdfs:range xsd:String ;
  rdfs:label "détails instrument d'analyse" .
  
:Materiaux a owl:Class ;
  rdfs:label "Matériaux" ;
  rdfs:comment "liste des Matériaux ou composants associés à une analyse." .

:ElementsChimiques a owl:Class ;
  rdfs:label "Éléments chimiques" ;
  rdfs:comment "list des Éléments chimiques recherchés ou identifiés." .

############################################################
# ANNEXES & MULTIMÉDIA
############################################################

:Annexe a owl:Class ;
  rdfs:label "Annexe" ;
  rdfs:comment "Annexe jointe à un rapport ou un devis." .

:MultimediaAnnexe a owl:Class ;
  rdfs:label "Ressource multimédia d'annexe" ;
  rdfs:comment "Image ou spectre joint à une annexe." .

############################################################
# OBJETS
############################################################

:Objet a owl:Class ;
  rdfs:label "Objet" ;
  rdfs:comment "Objet concerné par un devis ou une proposition commerciale." .

:descriptionObjet a owl:DatatypeProperty ;
  rdfs:domain :Objet ;
  rdfs:range xsd:string ;
  rdfs:label "description Objet" .

############################################################
# PRIX
############################################################

:Prix a owl:Class ;
  rdfs:label "Prix" ;
  rdfs:comment "Informations tarifaires associées à un devis." .

:FraisGestion a owl:Class ;
  rdfs:label "Frais de gestion" .

:FraisRetour a owl:Class ;
  rdfs:label "Frais de retour" .

############################################################
# LIVRABLES & LOGISTIQUE
############################################################

:Livrable a owl:Class ;
  rdfs:label "Livrable" .



############################################################
# CLAUSES
############################################################

:Clausesdanalyse a owl:Class ;
  rdfs:label "Clause de proposition" ;
  rdfs:comment "ClauseConfidentialite ,Clause de force majeure.".



:descriptionClause a owl:DatatypeProperty ;
  rdfs:domain :Clausesdanalyse;
  rdfs:range xsd:string ;
  rdfs:label "description clause" .
############################################################
# PROPRIÉTÉS OBJET
############################################################

:devisPourClient a owl:ObjectProperty ;
  rdfs:domain :Devis ;
  rdfs:range :Client ;
  rdfs:label "devis pour client" .

:propositionPourClient a owl:ObjectProperty ;
  rdfs:domain :PropositionCommerciale ;
  rdfs:range :Client ;
  rdfs:label "proposition pour client" .

:propositionEmisePar a owl:ObjectProperty ;
  rdfs:domain :PropositionCommerciale ;
  rdfs:range :Fournisseur ;
  rdfs:label "proposition émise par" .

:devisEmisPar a owl:ObjectProperty ;
  rdfs:domain :Devis ;
  rdfs:range :Fournisseur ;
  rdfs:label "Devis émise par" .

:devisContientObjet a owl:ObjectProperty ;
  rdfs:domain :Devis ;
  rdfs:range :Objet ;
  rdfs:label "contient objet" .

:propositionContientObjet a owl:ObjectProperty ;
  rdfs:domain :PropositionCommerciale ;
  rdfs:range :Objet ;
  rdfs:label "contient objet" .

:analysesProposeesParDevis a owl:ObjectProperty ;
  rdfs:domain :Devis ;
  rdfs:range :AnalysesProposees ;
  rdfs:label "analyses proposées par devis" .

:analysesProposeesParProposition a owl:ObjectProperty ;
  rdfs:domain :PropositionCommerciale ;
  rdfs:range :AnalysesProposees ;
  rdfs:label "analyses proposées par proposition" .

:analyseUtiliseInstrument a owl:ObjectProperty ;
  rdfs:domain :AnalysesProposees ;
  rdfs:range :InstrumentAnalyse ;
  rdfs:label "utilise instrument" .

:analyseUtiliseMateriaux a owl:ObjectProperty ;
  rdfs:domain :AnalysesProposees ;
  rdfs:range :Materiaux ;
  rdfs:label "utilise matériaux" .

:analyseInclutElementsChimiques a owl:ObjectProperty ;
  rdfs:domain :AnalysesProposees ;
  rdfs:range :ElementsChimiques ;
  rdfs:label "inclut éléments chimiques" .

:echantillonInclutElementsChimiques a owl:ObjectProperty ;
  rdfs:domain :Echantillon ;
  rdfs:range :ElementsChimiques ;
  rdfs:label "inclut éléments chimiques" .

:devisAUnPrix a owl:ObjectProperty ;
  rdfs:domain :Devis ;
  rdfs:range :Prix ;
  rdfs:label "a un prix" .

:PropositionCommercialeincluPrix a owl:ObjectProperty ;
  rdfs:domain :PropositionCommerciale ;
  rdfs:range :Prix ;
  rdfs:label "inclut prix" .

:prixInclutFraisGestion a owl:ObjectProperty ;
  rdfs:domain :Prix ;
  rdfs:range :FraisGestion ;
  rdfs:label "inclut frais de gestion" .

:prixInclutFraisRetour a owl:ObjectProperty ;
  rdfs:domain :Prix ;
  rdfs:range :FraisRetour ;
  rdfs:label "inclut frais de retour" .

:propositionInclutLivrable a owl:ObjectProperty ;
  rdfs:domain :PropositionCommerciale ;
  rdfs:range :Livrable ;
  rdfs:label "inclut livrable" .

:devisInclutLivrable a owl:ObjectProperty ;
  rdfs:domain :Devis ;
  rdfs:range :Livrable ;
  rdfs:label "inclut livrable" .

:echantillonEstAccompagnePar a owl:ObjectProperty ;
  rdfs:domain :Echantillon ;
  rdfs:range :BordereauLivraison , :BonLivraison ;
  rdfs:label "est accompagné par" .

:propositionImposeClause a owl:ObjectProperty ;
  rdfs:domain :PropositionCommerciale ;
  rdfs:range :ClauseConfidentialite , :ClauseForceMajeure ;
  rdfs:label "impose clause" .

:annexeContientRessource a owl:ObjectProperty ;
  rdfs:domain :Annexe ;
  rdfs:range :MultimediaAnnexe ;
  rdfs:label "contient ressource annexe" .

:corpsEtrangerObserveSur a owl:ObjectProperty ;
  rdfs:domain :CorpsEtranger ;
  rdfs:range :Echantillon;
  rdfs:label "corps étranger observé sur" ;
  rdfs:comment "Indique l’objet, l’échantillon ou l’élément constitutif sur lequel un corps étranger est détecté." .

:corpsEtrangerAnalysepar a owl:ObjectProperty ;
  rdfs:domain :CorpsEtranger ;
  rdfs:range :InstrumentAnalyse ;
  rdfs:label "corps étranger analysé par" ;
  rdfs:comment "corps étranger analysé par l'outil d'analyse ." .

:methodeDetectionCorpsEtranger a owl:DatatypeProperty ;
  rdfs:domain :CorpsEtranger ;
  rdfs:range xsd:string ;
  rdfs:label "méthode de détection du corps étranger" ;
  rdfs:comment "Méthode ou technique utilisée pour détecter le corps étranger (ex : MEB-EDS, ToF-SIMS, microscopie optique)." .


:typeCorpsEtranger a owl:DatatypeProperty ;
  rdfs:domain :CorpsEtranger ;
  rdfs:range xsd:string ;
  rdfs:label "type de corps étranger" ;
  rdfs:comment "Nature du corps étranger (ex : métallique, organique, polymère, poussière, fibre)." .

:compositionCorpsEtranger a owl:DatatypeProperty ;
  rdfs:domain :CorpsEtranger ;
  rdfs:range xsd:string ;
  rdfs:label "composition du corps étranger" ;
  rdfs:comment "Description textuelle ou synthèse de la composition chimique identifiée (ex : Al, Si, C, F)." .

