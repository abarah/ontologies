@prefix : <http://data.serma.org/ontoserma#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:DocumentMetier a owl:Class ;
    rdfs:label "Document métier" ;
    rdfs:comment "Classe générale représentant n'importe quel document interne du groupe Serma." .

:RapportAnalyse a owl:Class ;
    rdfs:label "Rapports d'analyse" ;
    rdfs:subClassOf :DocumentMetier ;
    rdfs:comment "Document qui synthétise les résultats d'analyses techniques." .

:Echantillon a owl:Class ;
    rdfs:label "Échantillon" ;
    rdfs:comment "Échantillon analysé (avec date d'analyse, référence, etc.)." .

:ProgrammeAnalyse a owl:Class ;
    rdfs:label "Programme d'analyse" ;
    rdfs:comment "Protocole ou méthodologie utilisée lors d'une analyse, dans un rapport ou un devis." .

:détailsProgrammeAnalyse a owl:DatatypeProperty ;
    rdfs:domain :ProgrammeAnalyse ; rdfs:range xsd:string ;
    rdfs:label "détails de programme ." .

:ParametreAnalyse a owl:Class ;
    rdfs:label "Paramètre d'analyse" ;
    rdfs:comment "Paramètre analytique mesuré ou contrôlé pendant l'analyse (ex. température, durée, concentration)." .

:descriptionParametreAnalyse a owl:DatatypeProperty ;
    rdfs:domain :ParametreAnalyse ; rdfs:range xsd:string ;
    rdfs:label "détails paramètre d'analyse ." .

:SpecificationInstrumentale a owl:Class ;
    rdfs:label "Spécification analytique ou instrumentale" ;
    rdfs:comment "Spécification liée à l'instrument ou à la méthode analytique (ex. aire analysée, résolution, grossissement)." .

:descriptionSpecificationInstrumentale a owl:DatatypeProperty ;
    rdfs:domain :SpecificationInstrumentale ; rdfs:range xsd:string ;
    rdfs:label "détails Spécification Instrument." .

:ConditionExperimentale a owl:Class ;
    rdfs:label "Condition expérimentale" ;
    rdfs:comment "Condition physique ou chimique dans laquelle l'analyse est réalisée (ex. atmosphère, pression, humidité, environnement)." .

:detailsExperimentale a owl:DatatypeProperty ;
    rdfs:domain :ConditionExperimentale ; rdfs:range xsd:string ;
    rdfs:label "détails conditions." .


:Resultat a owl:Class ;
    rdfs:label "Résultat" ;
    rdfs:comment "Résultat de l'analyse (texte, tableau, graphe, etc.)." .

:texteResultat a owl:DatatypeProperty ;
    rdfs:domain :Resultat ; rdfs:range xsd:string ;
    rdfs:label "text résultat" .

:Conclusion a owl:Class ;
    rdfs:label "Conclusion" ;
    rdfs:comment "Conclusion écrite tirée de l'analyse." .

:texteConclusion a owl:DatatypeProperty ;
    rdfs:domain :Conclusion ; rdfs:range xsd:string ;
    rdfs:label "text conclusion" .

:Annexe a owl:Class ;
    rdfs:label "Annexe" ;
    rdfs:comment "Annexe jointe au rapport contenant des images, spectres, profils, tableaux, etc." .

:MultimediaAnnexe a owl:Class ;
    rdfs:label "Ressource multimédia d'annexe" ;
    rdfs:comment "Image ou spectre joint à une annexe de rapport." .

:textRessource a owl:DatatypeProperty ;
    rdfs:domain :MultimediaAnnexe ;
    rdfs:range xsd:string ;
    rdfs:label "texte ressource" ;
    rdfs:comment "Texte descriptif associé à la ressource multimédia." .


:annexeContientRessource a owl:ObjectProperty ;
    rdfs:domain :Annexe ;
    rdfs:range :MultimediaAnnexe ;
    rdfs:label "contient ressource annexe" ;
    rdfs:comment "Ressources multimédias spécifiques au documents.".


:textRessourceObjet a owl:DatatypeProperty ;
    rdfs:subPropertyOf :textRessource ;
    rdfs:domain :MultimediaObjet ;
    rdfs:range xsd:string ;
    rdfs:label "texte ressource objet" ;
    rdfs:comment "Texte descriptif associé à la ressource multimédia d'objet." .

:textRessourceconclusion a owl:DatatypeProperty ;
    rdfs:subPropertyOf :textRessource ;
    rdfs:domain :MultimediaConclusion ;
    rdfs:range xsd:string ;
    rdfs:label "texte ressource conclusion" ;
    rdfs:comment "Texte descriptif associé à la ressource multimédia conclusion." .

:textRessourceResultat a owl:DatatypeProperty ;
    rdfs:subPropertyOf :textRessource ;
    rdfs:domain :MultimediaResultat ;
    rdfs:range xsd:string ;
    rdfs:label "texte ressource résultat" ;
    rdfs:comment "Texte descriptif associé à la ressource multimédia de résultat." .

:textRessourceAnnexe a owl:DatatypeProperty ;
    rdfs:subPropertyOf :textRessource ;
    rdfs:domain :MultimediaAnnexe ;
    rdfs:range xsd:string ;
    rdfs:label "texte ressource annexe" ;
    rdfs:comment "Texte descriptif associé à la ressource multimédia d'annexe." .

:rapportInclutEchantillon a owl:ObjectProperty ;
    rdfs:domain :RapportAnalyse ; rdfs:range :Echantillon ;
    rdfs:label "inclut échantillon" ;
    rdfs:comment "Lien entre un rapport et un échantillon analysé." .


:rapportInclutParametre a owl:ObjectProperty ;
  rdfs:domain :RapportAnalyse ;
  rdfs:range :ParametreAnalyse , :SpecificationInstrumentale , :ConditionExperimentale ;
  rdfs:label "inclut paramètre" ;
  rdfs:comment "Lien vers les paramètres utilisés lors de l'analyse." .

:rapportProduitResultat a owl:ObjectProperty ;
    rdfs:domain :RapportAnalyse ; rdfs:range :Resultat ;
    rdfs:label "produit résultat" ;
    rdfs:comment "Lien entre un rapport et les résultats générés." .

:rapportInclutConclusion a owl:ObjectProperty ;
    rdfs:domain :RapportAnalyse ; rdfs:range :Conclusion ;
    rdfs:label "inclut conclusion" ;
    rdfs:comment "Lien vers la conclusion du rapport." .



:rapportInclutAnnexe a owl:ObjectProperty ;
    rdfs:domain :RapportAnalyse ; rdfs:range :Annexe ;
    rdfs:label "inclut annexe" ;
    rdfs:comment "Lien vers les annexes du rapport." .




:responsableRapport a owl:DatatypeProperty ;
    rdfs:domain :RapportAnalyse ; rdfs:range xsd:string ;
    rdfs:label "responsable d'analyse." .

:dateAnalyse a owl:DatatypeProperty ;
    rdfs:domain :Echantillon ; rdfs:range xsd:dateTime ;
    rdfs:label "date d'analyse ." .



:dateReception a owl:DatatypeProperty ;
    rdfs:domain :Echantillon ; rdfs:range xsd:dateTime ;
    rdfs:label "date de réception" .

:referenceEchantillon a owl:DatatypeProperty ;
    rdfs:domain :Echantillon ; rdfs:range xsd:string ;
    rdfs:label "référence échantillon" .

:commentaireEchantillon a owl:DatatypeProperty ;
    rdfs:domain :Echantillon ; rdfs:range xsd:string ;
    rdfs:label "commentaire échantillon" .

:ObjetAnalyse a owl:Class ;
    rdfs:label "Objet d'analyse" ;
    rdfs:comment "Objet technique soumis à une analyse, dans un document." .





:ObjetRapport a owl:Class ;
    rdfs:label "Objet du rapport" ;
    rdfs:comment "Objet analysé dans le contexte d'un rapport d'analyse." .




:ObjetDevis a owl:Class ;
    rdfs:label "Objet du devis" ;
    rdfs:comment "Objet concerné par le rapport d'analyse." .



:rapportContientObjet a owl:ObjectProperty ;
    rdfs:domain :RapportAnalyse ;
    rdfs:range :ObjetRapport ;
    rdfs:label "contient objet analysé" ;
    rdfs:comment "Objet analysé dans un rapport." .

:devisContientObjet a owl:ObjectProperty ;
    rdfs:domain :Devis ;
    rdfs:range :ObjetDevis ;
    rdfs:label "contient objet devis" ;
    rdfs:comment "Objet concerné par un devis." .



:descriptionObjetRapport a owl:DatatypeProperty ;
    rdfs:domain :ObjetRapport ;
    rdfs:range xsd:string ;
    rdfs:label "description objet rapport" ;
    rdfs:comment "Texte descriptif associé à l'objet du rapport." .

:descriptionObjetDevis a owl:DatatypeProperty ;
    rdfs:subPropertyOf :descriptionobjet ;
    rdfs:domain :ObjetDevis ;
    rdfs:range xsd:string ;
    rdfs:label "description objet devis" ;
    rdfs:comment "Texte descriptif associé à l'objet du devis." .



:techniqueAnalyse a owl:DatatypeProperty ;
    rdfs:domain :ProgrammeAnalyse ; rdfs:range xsd:string ;
    rdfs:label "Texte descriptif associé au technique d'analyse" .

:methodologie a owl:DatatypeProperty ;
    rdfs:domain :ProgrammeAnalyse ; rdfs:range xsd:string ;
    rdfs:label "les détails du méthode d'analyse" .




:contenuAnnexe a owl:DatatypeProperty ;
    rdfs:domain :Annexe ; rdfs:range xsd:string ;
    rdfs:label "contenu annexe" .





:Devis a owl:Class ;
    rdfs:label "Devis" ;
    rdfs:subClassOf :DocumentMetier ;
    rdfs:comment "Document de type devis technique ou commercial." .

:Prix a owl:Class ;
    rdfs:label "Prix" ;
    rdfs:comment "Contient les informations tarifaires liées à un devis, que ce soit sous forme structurée ou non." .



:utiliseProgramme a owl:ObjectProperty ;
    rdfs:domain :RapportAnalyse;
    rdfs:range :ProgrammeAnalyse ;
    rdfs:label "utilise programme";
    rdfs:comment "Le document (rapport ou devis) utilise un programme ou protocole d'analyse.".

:devisAUnPrix a owl:ObjectProperty ;
    rdfs:domain :Devis ;
    rdfs:range :Prix ;
    rdfs:label "a un prix";
    rdfs:comment "Lien entre un devis et ses informations tarifaires.".


:prixTotal a owl:DatatypeProperty ;
    rdfs:domain :Prix ;
    rdfs:range xsd:decimal ;
    rdfs:label "prix total";
    rdfs:comment "Somme totale exprimée dans les informations tarifaires.".

:detailPrix a owl:DatatypeProperty ;
    rdfs:domain :Prix ;
    rdfs:range xsd:string ;
    rdfs:label "détail des prix";
    rdfs:comment "Description textuelle ou semi-structurée des postes de prix.".

:dateDeDevis a owl:DatatypeProperty ;
    rdfs:label "date de devis" ;
    rdfs:domain :Devis ;
    rdfs:range xsd:dateTime ;
    rdfs:comment "Date de création ou d'émission du devis." .

:versionDevis a owl:DatatypeProperty ;
    rdfs:label "version devis" ;
    rdfs:domain :Devis ;
    rdfs:range xsd:string ;
    rdfs:comment "Numéro de version ou de révision du devis." .


:Client a owl:Class ;
  rdfs:label "Client" ;
  rdfs:comment "Société client liée aux documents (devis, rapport d'analyse...). Contient les infos du responsable qui reçoit les documents." .



:devisPourClient a owl:ObjectProperty ;
  rdfs:domain :Devis ;
  rdfs:range :Client ;
  rdfs:label "devis pour client" ;
  rdfs:comment "Associe un devis à son client." .

:rapportPourClient a owl:ObjectProperty ;
  rdfs:domain :RapportAnalyse ;
  rdfs:range :Client ;
  rdfs:label "rapport pour client" ;
  rdfs:comment "Associe un rapport d'analyse à son client." .




:nomClient a owl:DatatypeProperty ;
    rdfs:domain :Client ;
    rdfs:range xsd:string ;
    rdfs:label "nom de la société" .

:email a owl:DatatypeProperty ;
    rdfs:domain :Client ;
    rdfs:range xsd:string ;
    rdfs:label "email de la société" .

:telephone a owl:DatatypeProperty ;
    rdfs:domain :Client ;
    rdfs:range xsd:string ;
    rdfs:label "téléphone" .

