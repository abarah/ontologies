@prefix :    <http://socrete.org/normes#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

#################################################################
# CLASSES — Normes & documents techniques (sans subclasses)
#################################################################

:DocumentNormatif a owl:Class ;
  rdfs:label "Document normatif" ;
  rdfs:comment "Document de référence (norme, guide, recommended practice, spécification, etc.)." .

:Norme a owl:Class ;
  rdfs:label "Norme" ;
  rdfs:comment "Standard / norme technique publiée par un organisme." .

:Guide a owl:Class ;
  rdfs:label "Guide" ;
  rdfs:comment "Guide technique (guidelines, guide de conception, guide de mise en œuvre)." .

:PratiqueRecommandee a owl:Class ;
  rdfs:label "Pratique recommandée" ;
  rdfs:comment "Recommended practice (ex: DNV-RP-...)." .

:RapportTechnique a owl:Class ;
  rdfs:label "Rapport technique" ;
  rdfs:comment "Technical report / document de support (TR, report, note technique)." .

:Specification a owl:Class ;
  rdfs:label "Spécification" ;
  rdfs:comment "Spécification technique (exigences détaillées, spécification d’essai, etc.)." .

:OrganismeNormalisation a owl:Class ;
  rdfs:label "Organisme de normalisation" ;
  rdfs:comment "Organisme/éditeur (ISO, ASTM, ASCE, DNV, NACE, RILEM, AFNOR/NF EN, Eurocode, etc.)." .

:Edition a owl:Class ;
  rdfs:label "Édition" ;
  rdfs:comment "Édition/version d’un document (année, amendement, corrigendum)." .

:Section a owl:Class ;
  rdfs:label "Section" ;
  rdfs:comment "Section/chapitre/clauses/annexe d’un document." .

:Annexe a owl:Class ;
  rdfs:label "Annexe" ;
  rdfs:comment "Annexe (normative ou informative)." .

:Terme a owl:Class ;
  rdfs:label "Terme" ;
  rdfs:comment "Terme défini dans le document." .

:Definition a owl:Class ;
  rdfs:label "Définition" ;
  rdfs:comment "Définition associée à un terme." .

:Exigence a owl:Class ;
  rdfs:label "Exigence" ;
  rdfs:comment "Exigence normative (shall/doit), contrainte, règle, obligation." .

:Recommandation a owl:Class ;
  rdfs:label "Recommandation" ;
  rdfs:comment "Recommandation (should/devrait), bonne pratique." .

:MethodeEssai a owl:Class ;
  rdfs:label "Méthode d’essai" ;
  rdfs:comment "Méthode/procédure d’essai, protocole, séquence de test." .

:Procedure a owl:Class ;
  rdfs:label "Procédure" ;
  rdfs:comment "Procédure opératoire (mise en œuvre, inspection, préparation, etc.)." .

:CritereAcceptation a owl:Class ;
  rdfs:label "Critère d’acceptation" ;
  rdfs:comment "Critères de conformité/acceptation (pass/fail, tolérances, seuils)." .

:Parametre a owl:Class ;
  rdfs:label "Paramètre" ;
  rdfs:comment "Paramètre technique (température, durée, concentration, charge, etc.)." .

:ValeurLimite a owl:Class ;
  rdfs:label "Valeur limite" ;
  rdfs:comment "Seuil/limite/valeur exigée associée à un paramètre." .

:Mesure a owl:Class ;
  rdfs:label "Mesure" ;
  rdfs:comment "Mesure/valeur observée (résultat mesuré, indicateur)." .

:Unite a owl:Class ;
  rdfs:label "Unité" ;
  rdfs:comment "Unité (SI, unités d’essai, etc.)." .

:Materiau a owl:Class ;
  rdfs:label "Matériau" ;
  rdfs:comment "Matériau concerné (acier, béton, polymère, etc.)." .

:Composant a owl:Class ;
  rdfs:label "Composant" ;
  rdfs:comment "Composant/élément (barre d’acier, carte PCB, cathode, etc.)." .

:Structure a owl:Class ;
  rdfs:label "Structure" ;
  rdfs:comment "Structure/système (ouvrage, équipement, système de protection, etc.)." .

:Environnement a owl:Class ;
  rdfs:label "Environnement" ;
  rdfs:comment "Environnement d’exposition (marine, atmosphérique, immersion, etc.)." .

:Formule a owl:Class ;
  rdfs:label "Formule" ;
  rdfs:comment "Formule/équation (calcul dimensionnement, critères, etc.)." .

:Figure a owl:Class ;
  rdfs:label "Figure" ;
  rdfs:comment "Figure/illustration/diagramme." .

:Tableau a owl:Class ;
  rdfs:label "Tableau" ;
  rdfs:comment "Tableau (données, limites, paramètres, classes…)." .

:Reference a owl:Class ;
  rdfs:label "Référence" ;
  rdfs:comment "Référence bibliographique ou normative citée." .

:Conformite a owl:Class ;
  rdfs:label "Conformité" ;
  rdfs:comment "Déclaration/état de conformité à une exigence." .

#################################################################
# CLASSES — Gouvernance / consultation (ISO, commentaires, révision)
#################################################################

:ComiteTechnique a owl:Class ;
  rdfs:label "Comité technique" ;
  rdfs:comment "Comité (TC/SC) responsable d’un document/projet." .

:GroupeDeTravail a owl:Class ;
  rdfs:label "Groupe de travail" ;
  rdfs:comment "Groupe de travail (WG) rattaché à un comité." .

:ProjetNorme a owl:Class ;
  rdfs:label "Projet de norme" ;
  rdfs:comment "Projet (WD/CD/DIS/FDIS) ou document en développement." .

:Consultation a owl:Class ;
  rdfs:label "Consultation" ;
  rdfs:comment "Période de consultation/ballot et collecte de commentaires." .

:Commentaire a owl:Class ;
  rdfs:label "Commentaire" ;
  rdfs:comment "Commentaire soumis lors d’une consultation sur un projet." .

:ObservationSecretariat a owl:Class ;
  rdfs:label "Observation du secrétariat" ;
  rdfs:comment "Observation/retour du secrétariat ou du comité sur un commentaire." .

:PropositionModification a owl:Class ;
  rdfs:label "Proposition de modification" ;
  rdfs:comment "Modification proposée (remplacement/suppression/ajout de texte)." .

:Decision a owl:Class ;
  rdfs:label "Décision" ;
  rdfs:comment "Décision sur un commentaire (accepté/rejeté/à étudier…)." .

:MembreNational a owl:Class ;
  rdfs:label "Membre national" ;
  rdfs:comment "National committee / member body auteur de commentaires." .

