#################################################################
# DATATYPE PROPERTIES — Métadonnées document
#################################################################

:titre a owl:DatatypeProperty ;
  rdfs:label "titre" ;
  rdfs:domain :DocumentNormatif ;
  rdfs:range xsd:string .

:identifiant a owl:DatatypeProperty ;
  rdfs:label "identifiant" ;
  rdfs:comment "Code du document (ex: ISO 5929, DNV-RP-B401, ASTM Gxx, EN 199x...)." ;
  rdfs:domain :DocumentNormatif ;
  rdfs:range xsd:string .

:langue a owl:DatatypeProperty ;
  rdfs:label "langue" ;
  rdfs:domain :DocumentNormatif ;
  rdfs:range xsd:string .

:datePublication a owl:DatatypeProperty ;
  rdfs:label "datePublication" ;
  rdfs:domain :Edition ;
  rdfs:range xsd:string .

:version a owl:DatatypeProperty ;
  rdfs:label "version" ;
  rdfs:comment "Édition, année, amendement, révision, etc." ;
  rdfs:domain :Edition ;
  rdfs:range xsd:string .

:statut a owl:DatatypeProperty ;
  rdfs:label "statut" ;
  rdfs:comment "Publié / projet (CD/DIS/FDIS) / retiré / remplacé..." ;
  rdfs:domain :DocumentNormatif ;
  rdfs:range xsd:string .

:domaineTechnique a owl:DatatypeProperty ;
  rdfs:label "domaineTechnique" ;
  rdfs:domain :DocumentNormatif ;
  rdfs:range xsd:string .

:resume a owl:DatatypeProperty ;
  rdfs:label "resume" ;
  rdfs:domain :DocumentNormatif ;
  rdfs:range xsd:string .

